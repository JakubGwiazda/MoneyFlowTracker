{
  "name": "MoneyFlowTracker",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development",
    "start:local": "ng serve --configuration=local",
    "start:e2e": "cross-env NG_APP_ENV=e2e ng serve --configuration=e2e",
    "start:prod": "ng serve --configuration=production",
    "build": "ng build --configuration=local",
    "build:local": "ng build --configuration=local",
    "build:prod": "ng build --configuration=production",
    "build:e2e": "ng build --configuration=e2e",
    "build:dev": "ng build --configuration=development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen http://localhost:4200",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json,css,md}\"",
    "db_start": "supabase start"
  },
  "dependencies": {
    "@angular/animations": "^20.3.7",
    "@angular/cdk": "^20.2.11",
    "@angular/common": "^20.3.7",
    "@angular/compiler": "^20.3.7",
    "@angular/core": "^20.3.7",
    "@angular/forms": "^20.3.7",
    "@angular/material": "^20.2.11",
    "@angular/platform-browser": "^20.3.7",
    "@angular/platform-browser-dynamic": "^20.3.7",
    "@angular/router": "^20.3.7",
    "@supabase/supabase-js": "^2.78.0",
    "@swimlane/ngx-charts": "^23.1.0",
    "bootstrap": "^5.3.8",
    "d3": "^7.9.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^20.0.0",
    "@angular-devkit/build-angular": "^20.3.7",
    "@angular/cli": "^20.3.12",
    "@angular/compiler-cli": "^20.3.7",
    "@eslint/compat": "1.2.7",
    "@eslint/js": "9.23.0",
    "@playwright/test": "^1.56.1",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "8.28.0",
    "@typescript-eslint/parser": "8.28.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "dotenv-webpack": "^8.1.1",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-import-resolver-typescript": "4.2.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.5",
    "husky": "9.1.7",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "15.5.0",
    "prettier": "^3.6.2",
    "supabase": "^2.54.11",
    "typescript-eslint": "8.28.0"
  },
  "lint-staged": {
    "*.{ts,html}": [
      "eslint --fix"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
